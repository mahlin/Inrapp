<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Socialstyrelsens inrapporteringsportal</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

<div class="logga">
    <img src="~/Content/Images/Logga.PNG" alt="Socialstyrelsen" style="height:100%">
</div>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("Inrapporteringsportal", "Index", "Home", new {area = ""}, new {@class = "navbar-brand"})

        </div>
        <div class="navbar-collapse collapse">
        </div>
    </div>
</div>
<div class="container body-content padding-top-55" id="contentBody">
    @RenderBody()
    <hr/>
    <footer>
        <p>&copy; @DateTime.Now.Year - Socialstyrelsens inrapporteringsportal</p>
    </footer>
</div>


    @Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</body>
</html>
